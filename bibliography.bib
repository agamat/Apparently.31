@article{Salhin2013,
abstract = {The impact of hard discount control mechanisms on the discount volatility of UK closed-end funds is investigated. Using ten years data, the paper starts by analyzing the main factors that influence the variation in discount volatility across the sector. Standard deviation of net asset value returns, market value and the percentage of unquoted assets in the fund’s portfolio are found to be highly significant. As a robustness test, the whole period is split into two five years periods which has no effect on the significance of the variables. The analysis is then extended to show that closed-end funds which commit to a hard discount control mechanism tend to have lower discount volatility. However, the last finding is not highly significant suggesting that in general hard discount control mechanisms could be used more effectively to control discount volatility.},
author = {Salhin, Ahmed},
file = {:Users/Ahmed/Library/Application Support/Mendeley Desktop/Downloaded/Salhin - 2013 - The impact of hard discount control mechanism on the discount volatility of UK closed-end funds.pdf:pdf},
